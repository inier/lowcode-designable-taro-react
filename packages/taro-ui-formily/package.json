{
  "name": "taro-ui-formily",
  "version": "0.0.1",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "scripts": {
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:rollup": "rollup --config ./config/rollup.config.js",
    "dev:lib": "tsc --watch --project ./tsconfig.build.json",
    "dev:rollup": "rollup --watch --config ./config/rollup.config.js"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "taro-ui": "3.1.0-beta.2"
  },
  "devDependencies": {
    "@tarojs/components": "3.5.0-beta.4",
    "@tarojs/react": "3.5.0-beta.4",
    "@tarojs/taro": "3.5.0-beta.4",
    "@types/classnames": "^2.2.9",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "prettier": "^2.6.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "peerDependencies": {
    "@formily/core": ">2.0.13",
    "@formily/react": ">2.0.13",
    "@tarojs/components": ">=3",
    "@tarojs/react": ">=3",
    "classnames": "^2.2.6",
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0",
    "taro-ui": "3.1.0-beta.2"
  }
}
